{
    "name": "nodes/validation",
    "description": "Makes it easier to work with validation in Laravel",
    "keywords": [
        "nodes",
        "validation",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "http://nodesagency.com",
    "authors": [
        {
            "name": "Morten Rugaard",
            "email": "moru@nodes.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "^9.0",
        "nodes/core": "dev-php-8-1"
    },
    "autoload": {
        "psr-4": {
            "Nodes\\Validation\\": "src"
        },
        "files": [
            "src/Support/Helpers/Checks.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nodes\\Validation\\ServiceProvider"
            ]
        }
    },
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/jamesforddesign/core"
    }
    ]
}
